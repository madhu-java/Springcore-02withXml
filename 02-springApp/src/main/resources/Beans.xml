<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="java" class="com.madhu.beans.JavaCourse"></bean>
	<bean id="sql" class="com.madhu.beans.SqlCourse"></bean>
	<bean id="py" class="com.madhu.beans.PythonCourse"></bean>
	<bean id="co" class="com.madhu.beans.CourseSelection">
		<property name="course" ref="java"></property>
	</bean>

</beans>